		"src/Red.tsx",
		"src/store.ts",
		"src/util/ContentTransition.tsx",
		"src/util/SwapTransition.tsx",
		"typings/gsap.d.ts",
		"typings/react/react-dom.d.ts",
		"typings/react/react.d.ts"
	]
}
